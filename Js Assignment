// Question 39 //
const make_album = (artistName, albumTitle, tracks = 0) => {
    album = {
        "artist": artistName,
        "album": albumTitle,
        "tracks": tracks
    }

    return album;
}

var callAlbum = make_album("ZaynMalik", "One Direction");
console.log(callAlbum)
var callAlbum = make_album("HarryStyles", "One Direction");
console.log(callAlbum)
var callAlbum = make_album("EdSheeran", "Fly HIgh", tracks = 8);
console.log(callAlbum)

