var greetingLIst = ["Admin", "Eric", "John", "Michael", "Mousa"];
for (let i = 0; i <= greetingLIst.length; i++){
    if(greetingLIst[i] == "Admin"){
    console.log("Hello " +greetingLIst[0] +", would you like to see a status report?")}
    else if ( greetingLIst[i]=="Eric"){
        console.log("Hello "+ greetingLIst[1]+ " thank you for logging again")
    } else if ( greetingLIst[i]=="John"){
        console.log("Hello "+ greetingLIst[2]+ " thank you for logging again")
    } else if ( greetingLIst[i]=="Michael"){
        console.log("Hello "+ greetingLIst[3]+ " thank you for logging again")
    } else if ( greetingLIst[i]=="Mousa"){
        console.log("Hello "+ greetingLIst[4]+ " thank you for logging again")
    }
} 



